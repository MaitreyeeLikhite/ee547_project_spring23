<!DOCTYPE html>
<html>
<head>
  <title>User Details</title>
  <style>
    table {
      border-collapse: collapse;
      width: 100%;
    }

    th, td {
      padding: 8px;
      text-align: left;
      border-bottom: 1px solid #ddd;
    }
  </style>
</head>
<body>
  <h1>User Details</h1>
  <table>
    <tr>
      <th>Name</th>
      <th>Email</th>
      <th>Phone</th>
      <th>Password</th>
      <th>Maintenance Data</th>
    </tr>
    <% users.forEach((user) => { %>
      <tr>
        <td><%= user.name %></td>
        <td><%= user.email %></td>
        <td><%= user.phone %></td>
        <td><%= user.password %></td>
        <td>
            <% if (user.data && user.data.length > 0) { %>
              <ul>
                <% user.data.forEach((data) => { %>
                  <li><%= data %></li>
                <% }) %>
              </ul>
            <% } else { %>
              <p>No data available</p>
            <% } %>
          </td>
      </tr>
    <% }); %>
  </table>

  <form method="POST" action="/logout">
    <button type="submit">Logout</button>
  </form>
</body>
</html>

</html>
